{
  "private": true,
  "engines": {
    "node": "^14.20"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3000 --dotenv .env.development",
    "dev:heaven": "nuxt dev --port 3001 --dotenv .env.development",
    "staging": "nuxt dev --port 3000 --dotenv .env.staging",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean-branches": "git branch | grep -v 'main' | xargs git branch -D",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/image-edge": "^1.0.0-27919678.2f5b64b",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
