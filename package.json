    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.19",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.1.2",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5",
    "vue-eslint-parser": "^9.1.0"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.3.1",
    "@pinia/nuxt": "^0.4.6",
